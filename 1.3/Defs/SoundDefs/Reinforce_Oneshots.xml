  </SoundDef>

  <SoundDef>
    <defName>Reinforce_FailedNormal</defName>
    <maxSimultaneous>2</maxSimultaneous>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Impact/BuildingDestroyed/Metal/Medium</clipFolderPath>
          </li>
        </grains>
        <volumeRange>30~30</volumeRange>
        <onCamera>true</onCamera>
      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
    <defName>Reinforce_FailedMinor</defName>
